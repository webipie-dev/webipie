{
  "name": "savvy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path=dist/savvy --baseHref / && ng build --prod --i18n-locale fr --i18n-format xlf --i18n-file src/locale/messages.fr.xlf --output-path=dist/fr --baseHref /fr/",
    "test": "jest",
    "test -watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.1",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.1",
    "@angular/compiler": "~10.2.1",
    "@angular/core": "~10.2.1",
    "@angular/forms": "~10.2.1",
    "@angular/localize": "^10.2.1",
    "@angular/platform-browser": "~10.2.1",
    "@angular/platform-browser-dynamic": "~10.2.1",
    "@angular/router": "~10.2.1",
    "@auth0/angular-jwt": "^5.0.1",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@mobiscroll/angular": "https://npm.mobiscroll.com/@mobiscroll/angular-trial/-/angular-trial-4.10.8.tgz",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@swimlane/ngx-charts": "^16.0.0",
    "angularx-social-login": "^3.3.1",
    "bootstrap": "^4.5.3",
    "encrypt-storage": "^1.0.11",
    "flag-icon-css": "^3.5.0",
    "jquery": "^3.5.1",
    "ng-image-slider": "^2.6.4",
    "ng-sidebar": "^9.4.2",
    "ng2-completer": "^9.0.1",
    "ng2-smart-table": "^1.7.0",
    "ngx-quill": "^12.0.1",
    "ngx-spinner": "^10.0.1",
    "quill": "^1.3.7",
    "rxjs": "~6.5.5",
    "socket.io-client": "^3.1.1",
    "sweetalert2": "^10.14.0",
    "tslib": "^2.0.0",
    "unique-slug": "^2.0.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.19.3",
    "codelyzer": "^6.0.0-next.1",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },

  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/backend/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
}
}
